Script.js
import books from "./data.js";
const cityInput = document.getElementById("cityInput");
const bookResults = document.getElementById('bookResults');
const searchBook = document.getElementById("searchBook");




// Function to display books based on the city name entered
function displayBooks(city) {
  // Filter books based on whether the city is listed in the location
  const filteredBooks = books.filter(book => book.location[city]);

  // Clear previous results
  bookResults.innerHTML = '';

  // If no books are found, display a "No data found" message
  if (filteredBooks.length === 0) {
    bookResults.innerHTML = `<p>No data found for books in "${city}".</p>`;
  } else {
    // Display the filtered books
    filteredBooks.forEach(book => {
      const bookElement = document.createElement('div');
      bookElement.classList.add('book');

      // Create HTML for each book
      bookElement.innerHTML = `
        <img src="${book.img_url}" alt="${book.name}" width="100">
       <div class="cardHeader"> <h3>${book.name}</h3>
        <p>${book.author_name}</p></div>
        <p id="price">Price: ${book.price}</p>
        <p>Category: ${book.category}</p>
        <p id="status">Status in ${city}: ${book.location[city]}</p>
      `;

      // Append the book to the results container
      bookResults.appendChild(bookElement);
    });
  }
}

function displayBook(bookName)
{
    const filteredBook = books.filter((book)=>book.name == bookName);
    bookResults.innerHTML ="";
    if(filteredBook.length===0)
    {
        bookResults.innerHTML = `<p>No data found for "${bookName}".</p>`;
    }
    else{
      console.log(filteredBook);
      const bookElement = document.createElement('div');
      bookElement.classList.add('book');

    //   // Create HTML for each book
      bookElement.innerHTML = `
        <img src="${filteredBook[0].img_url}" alt="${filteredBook[0].name}" width="100">
       <div class="cardHeader"> <h3>${filteredBook[0].name}</h3>
        <p>${filteredBook[0].author_name}</p></div>
        <p id="price">Price: ${filteredBook[0].price}</p>
        <p>Category: ${filteredBook[0].category}</p>
        <p id="status">Status : ${filteredBook[0].location}</p>
      `;

      // Append the book to the results container
      bookResults.appendChild(bookElement);
    }
}

// Event listener to trigger book display on input change
cityInput.addEventListener('input', () => {
  const city = cityInput.value.trim(); // Get the input value (city)
  if (city) {
    displayBooks(city);
  } 
});

searchBook.addEventListener('input',()=>{
    const book = searchBook.value.trim();
    if(book)
    {
        displayBook(book);
    }
})

